{
    "name": "xtwoend/oauth-server",
    "description": "OAuth2 Server for Hyperf Framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdul Hafidz Anshari",
            "email": "aditans88@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "hyperf/database": "^2.2",
        "hyperf/db-connection": "^2.2",
        "league/oauth2-server": "^8.2",
        "nesbot/carbon": "^2.0",
        "phpseclib/phpseclib": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "OAuthServer\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        },
        "hyperf": {
            "config": "OAuthServer\\ConfigProvider"
        }
    }
}
